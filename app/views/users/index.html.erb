<div class="container-fluid">
<h1>Users</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Admin?</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.admin? %></td>
        <td><%= link_to 'Edit', edit_user_registration_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>